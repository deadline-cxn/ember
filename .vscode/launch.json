        {
            "name": "(gdb) Linux Client Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/client/ember_client",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/client",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
