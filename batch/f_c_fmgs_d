echo ":s_vars.cpp"
g++ -D_FMSERVER_ -DFM_FREEBSD -c -g s_vars.cpp

echo ":servernet.cpp"
g++ -D_FMSERVER_ -DFM_FREEBSD -c -g servernet.cpp

echo " "
echo "Done compiling..."
echo " "
echo "Linking FMGS (Debug)..."
g++ *.o -o- g fmgs_debug



